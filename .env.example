ZIP_PATH=path/to/your/bundle.zip
ENV=YOUR_ENV # dev|staging|prod

TF_VAR_secret_name=YOUR_SECRETS_MANAGER_NAME
TF_VAR_project=YOUR_PROJECT_SLUG
TF_VAR_region=YOUR_AWS_REGION # e.g., eu-west-3
TF_VAR_table_name=YOUR_DYNAMODB_TABLE_NAME
TF_VAR_role_name=YOUR_LAMBDA_EXEC_ROLE_NAME
TF_VAR_func_name=YOUR_LAMBDA_FUNCTION_NAME
TF_VAR_api_name=YOUR_API_GATEWAY_NAME
TF_VAR_kms_alias=alias/YOUR_KMS_ALIAS # e.g., alias/your-secrets
TF_VAR_log_retention_days=14

CB_BASE_URL=https://YOUR_COINBASE_EXCHANGE_BASE_URL # e.g., https://api-public.sandbox.exchange.coinbase.com
API_URL=https://YOUR_API_GATEWAY_INVOKE_URL/ # note trailing slash if your app expects it
WEB_BUCKET=YOUR_S3_WEBSITE_BUCKET
CLOUDFRONT_DOMAIN=YOUR_CLOUDFRONT_DOMAIN # e.g., abcdef123456.cloudfront.net
CLOUDFRONT_ID=YOUR_CLOUDFRONT_DISTRIBUTION_ID
WEB_ORIGIN=https://YOUR_WEB_ORIGIN # e.g., https://abcdef123456.cloudfront.net

VITE_API_BASE=https://YOUR_API_GATEWAY_BASE_URL
TF_VAR_web_origin=https://YOUR_WEB_ORIGIN
TF_VAR_enable_jwt_authorization=true # or false

VITE_COGNITO_REGION=YOUR_COGNITO_REGION
VITE_COGNITO_REDIRECT_URI=https://YOUR_WEB_ORIGIN/auth/callback
VITE_COGNITO_CLIENT_ID=YOUR_COGNITO_APP_CLIENT_ID
VITE_COGNITO_DOMAIN=YOUR_COGNITO_DOMAIN # e.g., your-app.auth.eu-west-3.amazoncognito.com
VITE_COGNITO_LOGOUT_URI=https://YOUR_WEB_ORIGIN/login
